//This code is for head of linked list.
#include <bits/stdc++.h>
#include<stdio.h>
#include<stdlib.h>

using namespace std;
struct node{
    int data;
    struct node *link;//self refrential structure
};

int main()
{
   struct node *head =NULL;
   head=(struct node *)malloc(sizeof(struct node));//this is not mandatory but a good practice

   head->data =45;
   head->link =NULL;
   printf("%d",head->data);
   return 0;
}

